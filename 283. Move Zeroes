public class Solution {
    public void MoveZeroes(int[] nums)
    {
      int nonZeroElem =0; 
        
      for (int i= 0 ; i<nums.Length;i++)  {
         if (nums[i]!=0)
           nums[nonZeroElem++] = nums[i]; 
      }
        for (int i = nonZeroElem ; i< nums.Length;i++)
            nums[i]=0;
    }
}
